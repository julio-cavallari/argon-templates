{
    "name": "julio-cavallari/argon-templates",
    "type": "library",
    "description": "Argon templates for InfyOm Laravel Generator",
    "keywords": [
        "infyom",
        "laravel",
        "generator",
        "argon",
        "templates",
        "bootstrap"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Julio Cavallari",
            "email": "cesinha--jcf@hotmail.com"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "illuminate/support": "^8.67.0"
    },
    "autoload": {
        "psr-4": {
            "InfyOm\\ArgonTemplates\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "\\InfyOm\\ArgonTemplates\\ArgonTemplatesServiceProvider"
            ]
        }
    }
}
